---
layout: nil
---

<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/atom">
	<channel>
		<title>{{ site.title }}</title>
		<description>{{ site.description }}</description>
		<link>http://viralsyndrome.github.io</link>
		<atom:link href="http://viralsyndrome.github.io/rss.xml" rel="self" type="application/rss+xml" />
		<language>en-us</language>
		<pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
		<lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

		{% for article in site.articles %}
		<item>
			<title>{{ article.title }}</title>
			<link>http://viralsyndrome.github.io{{ article.url }}</link>
			<pubDate>{{ article.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
			<author>Viral Syndrome</author>
			<guid>http://viralsyndrome.github.io{{ article.id }}</guid>
			<description>{{ article.content | xml_escape }}</description>
		</item>
		{% endfor %}
	</channel>
</rss>
